<collectionService
    xmlns="http://chs.harvard.edu/xmlns/cite"
    xmlns:cite="http://chs.harvard.edu/xmlns/cite">
    <citeCollection
        canonicalId="URN"
        class="1GC4S75Vz9GI6sYcLXGWTvgrK7-usKB_eU9MGoGQ"
        description="Mapping of Pleiades project IDs to RAGE project URNs"
        name="ptolgeo">
        <namespaceMapping
            abbr="rage"
            fullValue="http://shot.holycross.edu/datans/rage"/>
        <citeProperty
            label="URN"
            name="URN"
            type="citeurn"/>
        <citeProperty name="label" label="Label" type="string"/>
        <citeProperty type="number" label="Pleiades ID" name="PleiadesId"></citeProperty>
    </citeCollection>
    <citeCollection
        canonicalId="URN"
        class="1PMaa_kAegdo8sZ9QqjeN79lnL9zFXXlnxFHwRZ8"
        description="Homer Multitext project: the Venetus A manuscript"
        name="msA">
        <namespaceMapping
            abbr="hmt"
            fullValue="http://www.homermultitext.org/datans"/>
        <orderedBy
            property="Sequence"/>
        <citeProperty
            label="URN"
            name="URN"
            type="citeurn"/>
        <citeProperty
            label="Sequence"
            name="Sequence"
            type="number"/>
        <citeProperty
            label="recto or verso"
            name="RV"
            type="string">
            <valueList>
                <value>recto</value>
                <value>verso</value>
            </valueList>
        </citeProperty>
        <citeProperty
            label="Label"
            name="Label"
            type="string"/>
        <citeProperty
            label="Codex"
            name="CodexURN"
            type="citeurn"/>
    </citeCollection>
    <citeCollection
        canonicalId="URN"
        class="1tFhVfSS_u5zokh3EcIhjEwPEKjTfAwnHkEMPQD8"
        description="The Drei MÃ¤nner Kommentar to the Iliad"
        name="DMK">
        <namespaceMapping
            abbr="hmt"
            fullValue="http://www.homermultitext.org/datans"/>
        <citeProperty
            label="URN"
            name="URN"
            type="citeurn"/>
        <!-- maps to Google String -->
        <citeProperty
            label="URN of text commented on"
            name="URN-commentedOn"
            type="citeurn"/>
        <citeProperty
            label="Author"
            name="Author"
            type="string"/>
        <citeProperty
            label="Comment"
            name="Comment"
            type="markdown"/>
        <!-- maps to Google String -->
        <citeProperty
            label="Date"
            name="Date"
            type="datetime"/>
    </citeCollection>
    <citeCollection
        canonicalId="URN"
        class="1iElXI_3c0owUsTBw5Ao0xv8FZ7nf6MsHbwX_B9E"
        description="Homer Multitext project at Holy Cross:  work log"
        name="hmtWorkLog">
        <namespaceMapping
            abbr="hmt"
            fullValue="http://www.homermultitext.org/datans"/>
        <citeProperty
            label="URN"
            name="URN"
            type="citeurn"/>
        <!-- maps to Google String -->
        <citeProperty
            label="URN of object commented on"
            name="URN-commentedOn"
            type="citeurn"/>
        <citeProperty
            label="Author"
            name="Author"
            type="string"/>
        <citeProperty
            label="Comment"
            name="Comment"
            type="markdown"/>
        <!-- maps to Google String -->
        <citeProperty
            label="Date"
            name="Date"
            type="datetime"/>
    </citeCollection>
    <citeCollection
        canonicalId="URN"
        class="1Yt3tSWh2C5-t9y5-3I9SauZhljUgmmzoPa_6xQI"
        description="Open Paleography project:  Greek paleography"
        name="greek">
        <namespaceMapping
            abbr="paleog"
            fullValue="http://shot.holycross.edu/datans/paleography"/>
        <citeProperty
            label="URN"
            name="URN"
            type="citeurn"/>
        <citeProperty
            label="Image citation"
            name="ImageUrn"
            type="citeimg"/>
        <citeProperty
            label="Description"
            name="Description"
            type="string"/>
        <citeProperty
            label="Type of feature"
            name="RepertoryType"
            type="string">
            <valueList>
                <value>letter</value>
                <value>abbreviation</value>
                <value>ligature</value>
                <value>punctuation</value>
            </valueList>
        </citeProperty>
        <citeProperty
            label="Physical artifact"
            name="PhysicalArtifact"
            type="citeurn"/>
        <citeProperty
            label="Visual class"
            name="VisualClass"
            type="string">
            <valueList>
                <value>Suspension</value>
                <value>Horizontal</value>
                <value>Vertical</value>
                <value>Slanting</value>
                <value>Sinusoid</value>
                <value>Superscription</value>
                <value>Discontinuous</value>
                <value>Monograms</value>
                <value>Symbols</value>
                <value>Brachigraphy</value>
            </valueList>
        </citeProperty>
        <citeProperty
            label="Contributor"
            name="Contributor"
            type="string"/>
    </citeCollection>
</collectionService>
