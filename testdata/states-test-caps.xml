<collectionService
    xmlns="http://chs.harvard.edu/xmlns/cite"
    xmlns:cite="http://chs.harvard.edu/xmlns/cite">
    <citeCollection
        canonicalId="URN"
        class="1ah-wUkhbcqVkfNgbixS0eQFUTd52Md25HlYL4Cs"
        description="Small test collection on US States"
        name="states">
        <namespaceMapping
            abbr="ussstates"
            fullValue="http://shot.holycross.edu/datans/ussstates"/>
        <citeProperty
            label="URN"
            name="StateURN"
            type="citeurn"/>
        <citeProperty label="Name of state" name="State" type="string"/>
        <citeProperty
            label="Capital city"
            name="Capital city"
            type="string"/>

        <citeProperty
            label="Geographic location of capital"
            name="Location of Capital"
            type="string"/>

        <citeProperty
            label="Population, 2000 census"
	   name="Population, 2000 census"
            type="string"/>

        <citeProperty
            label="Date of statehood"
            name="Date of statehood"
	   type="datetime" />


    </citeCollection>
</collectionService>
